<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appsearchalogrithm"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.appsearchalogrithm.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.options.BubbleSortActivity"
            android:label="@string/title_activity_bubble_sort" >
        </activity>
        <activity
            android:name="com.example.options.SelectionSortActivity"
            android:label="@string/title_activity_selection_sort" >
        </activity>
        <activity
            android:name="com.example.options.InsertionSortActivity"
            android:label="@string/title_activity_insertion_sort" >
        </activity>
        <activity
            android:name="com.example.options.BucketSortActivity"    
            android:label="@string/title_activity_bucket_sort" >
        </activity>
        <activity
            android:name="com.example.options.RadixSortActivity"
            android:label="@string/title_activity_radix_sort" >
        </activity>
        <activity
            android:name="com.example.options.QuickSortActivity"
            android:label="@string/title_activity_quick_sort" >
        </activity>
        <activity
            android:name="com.example.options.MergeSortActivity"
            android:label="@string/title_activity_merge_sort" >
        </activity>
        <activity
            android:name="com.example.options.HeapSortActivity"    
            android:label="@string/title_activity_heap_sort" >
        </activity>
    </application>

</manifest>
